type Project {
  id: ID!
  name: String!
  description: String
  flow: String!
  typeConfig: String!
  owner: User!
  createdAt: String!
  updatedAt: String!
  shared: String
  sharedaccess: String
  cloneable : Boolean 
}

type User {
  name: String!
  picture: String
}

type Query {
  projects(ownerID: String!): [Project]!
  project(id: String!): Project
  publicprojects: [Project!]!
}
